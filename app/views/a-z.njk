{% extends "layout.njk" %}

{% set title = "A to Z" %}

{% block content %}
  <h1 class="govuk-heading-xl">{{ title }}</h1>

  {%- for index in aToZ -%}
  <h2 class="govuk-heading-l" id="#{{ index.slug }}">
    {{- index.name -}}
  </h2>

  <ul class="govuk-list govuk-list--spaced">
  {%- for service in index.services -%}
    {{ appServiceItem(service) }}
  {%- endfor %}
  </ul>
  {%- endfor %}
{% endblock %}
